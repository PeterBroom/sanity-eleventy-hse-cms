---
layout: layouts/base.njk
templateClass: tmpl-page
---
{% set components = pages.pageBuilder %}

<div class="content-container">
  <div data-column="one-quarter">
    {% include '../components/subnavPage.njk' %}
  </div>
  <div data-column="three-quarters">
    <h1>
      <abbr title="{{pages.pageTitleAccronym.accronymReplace}}">{{pages.pageTitleAccronym.accronymFind}}</abbr>
      {% filter replace(pages.accronymFind,accr) -%}
        {{pages.title | safe }}
      {%- endfilter %}
    </h1>
    {% include "../components/pageBuilder.njk" %}
  </div>
</div>